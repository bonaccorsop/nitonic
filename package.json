{
  "name": "nitonic",
  "version": "1.0.0",
  "description": "Fatturare forfettariamente in modo smart",
  "main": "cli.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && npm run includeSql && chmod +x dist/cli.js",
